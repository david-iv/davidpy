{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "olorireadf"
		},
		"APIRest_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'APIRest'"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"BlobLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobLinkedService'"
		},
		"SQLLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLLinkedService'"
		},
		"SQLLinkedservice2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLLinkedservice2'"
		},
		"jsonblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'jsonblob'"
		},
		"APIRest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.genderize.io?name=luc"
		},
		"APIRest_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "olorireserver"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "olorireadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"APIRest_password": {
						"value": "[parameters('APIRest_password')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"BlobLinkedService_connectionString": {
						"value": "[parameters('BlobLinkedService_connectionString')]"
					},
					"SQLLinkedService_connectionString": {
						"value": "[parameters('SQLLinkedService_connectionString')]"
					},
					"SQLLinkedservice2_connectionString": {
						"value": "[parameters('SQLLinkedservice2_connectionString')]"
					},
					"jsonblob_connectionString": {
						"value": "[parameters('jsonblob_connectionString')]"
					},
					"APIRest_properties_typeProperties_url": {
						"value": "[parameters('APIRest_properties_typeProperties_url')]"
					},
					"APIRest_properties_typeProperties_userName": {
						"value": "[parameters('APIRest_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}